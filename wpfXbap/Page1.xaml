<Page x:Class="wpfXbap.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      mc:Ignorable="d" Title="Page1" Height="600" Width="995" LostFocus="txbMaxNodeSt_LostFocus">
    <Grid Width="996" Height="600">
        <TextBlock Height="40" HorizontalAlignment="Left" Margin="252,12,0,0" Name="textBlock1" Text="Cops And Robbers THE GAME" VerticalAlignment="Top" Width="504" FontSize="24" TextAlignment="Center" FontWeight="Bold" FontStyle="Italic" />
        <Grid Height="328" HorizontalAlignment="Left" Margin="48,80,0,0" Name="grid1" VerticalAlignment="Top" Width="296" Background="#FFEFC1C1">
            <TextBox Height="23" HorizontalAlignment="Left" Margin="144,44,0,0" Name="txbGraphWidth" VerticalAlignment="Top" Width="120" Background="LightGray" LostFocus="txbGraphWidth_LostFocus" DataContext="{Binding}" />
            <Label Content="Szerokość planszy" Height="28" HorizontalAlignment="Left" Margin="28,44,0,0" Name="label4" VerticalAlignment="Top" />
            <Label Content="Wysokość planszy" Height="28" HorizontalAlignment="Left" Margin="28,78,0,0" Name="label5" VerticalAlignment="Top" IsEnabled="True" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="144,78,0,0" Name="txbGraphHeight" VerticalAlignment="Top" Width="120" Background="LightGray" LostFocus="txbGraphHeight_LostFocus" />
            <Button Content="Stwórz" Height="23" HorizontalAlignment="Left" Margin="108,223,0,0" Name="btnDrawGraph" VerticalAlignment="Top" Width="75" Click="btnDrawGraph_Click" />
            <Label Content="Rodzaj grafu" Height="28" HorizontalAlignment="Left" Margin="28,141,0,0" Name="label6" VerticalAlignment="Top" />
            <ComboBox Height="23" HorizontalAlignment="Left" Margin="144,145,0,0" Name="cmbEdgeType" VerticalAlignment="Top" Width="120" ItemsSource="{Binding}" />
            <ComboBox Height="23" HorizontalAlignment="Left" Margin="144,178,0,0" Name="cmbAlgorithm" VerticalAlignment="Top" Width="120" ItemsSource="{Binding}"/>
            <Label Content="Play yourself" Height="32" HorizontalAlignment="Left" Margin="78,6,0,0" Name="label1" VerticalAlignment="Top" Width="136" FontSize="16" FontWeight="Bold" />
            <Label Content="maxymalne wymiary 20x14" Height="28" HorizontalAlignment="Left" Margin="66,111,0,0" Name="label11" VerticalAlignment="Top" Width="166" />
            <Label Content="Algorytm ucieczki" Height="28" HorizontalAlignment="Left" Margin="29,178,0,0" Name="label13" VerticalAlignment="Top" />
        </Grid>
        <Grid Height="473" HorizontalAlignment="Left" Margin="397,80,0,0" Name="grid2" VerticalAlignment="Top" Width="583" Background="#FF65CB65">
            <Border BorderBrush="Silver" BorderThickness="1" Height="129" HorizontalAlignment="Left" Margin="25,178,0,0" Name="border1" VerticalAlignment="Top" Width="545" />
            <Border BorderBrush="Silver" BorderThickness="1" Height="62" HorizontalAlignment="Left" Margin="26,305,0,0" Name="border2" VerticalAlignment="Top" Width="544" />
            <TextBox x:Name="txbAlfaBetaDepthMin" Width="72" Height="25" TextWrapping="Wrap" Margin="289,300,122,98" />
            <Label Content="Test section" Height="34" HorizontalAlignment="Left" Margin="227,6,0,0" Name="label2" VerticalAlignment="Top" Width="138" FontSize="16" FontWeight="Bold" d:LayoutOverrides="HorizontalAlignment" />
            <Label Content="Ilość wierzchołków" Height="26" HorizontalAlignment="Left" Margin="25,67,0,0" Name="label3" VerticalAlignment="Top" Width="125" />
            <TextBox Height="32" HorizontalAlignment="Left" Margin="178,61,0,0" Name="txbNodeNumberMin" VerticalAlignment="Top" Width="91" Text="" LostFocus="txbNodeNumberMin_LostFocus" d:LayoutOverrides="HorizontalAlignment" />
            <TextBox Height="32" HorizontalAlignment="Left" Margin="401,61,0,0" Name="txbNodeNumberMax" Text="" VerticalAlignment="Top" Width="91" LostFocus="txbNodeNumberMax_LostFocus" />
            <TextBox Height="29" HorizontalAlignment="Left" Margin="178,109,0,0" Name="txbMaxNodeSt" VerticalAlignment="Top" Width="91" LostFocus="txbMaxNodeSt_LostFocus" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" />
            <Label Content="Ilość testów na graf" Height="29" Margin="278,110,182,0" Name="label7" VerticalAlignment="Top" d:LayoutOverrides="VerticalAlignment" />
            <TextBox Height="29" HorizontalAlignment="Left" Margin="401,110,0,0" Name="txbTestNumber" VerticalAlignment="Top" Width="91" LostFocus="txbTestNumber_LostFocus" d:LayoutOverrides="VerticalAlignment" />
            <Button Content="Start testing" Height="23" Margin="227.097,423,280.903,0" Name="btnStartTests" VerticalAlignment="Top" Click="btnStartTests_Click" />
            <Label Content="od" Height="28" HorizontalAlignment="Left" Margin="150,67,0,0" Name="label8" VerticalAlignment="Top" />
            <Label Content="do" Height="28" HorizontalAlignment="Left" Margin="325.903,65,0,0" Name="label9" VerticalAlignment="Top" RenderTransformOrigin="1.577,0.571" />
            <Label Content="max stopien wierz." Height="28" HorizontalAlignment="Left" Margin="25,110,0,0" Name="label10" VerticalAlignment="Top" d:LayoutOverrides="VerticalAlignment" />
            <CheckBox x:Name="chbGreedy" HorizontalAlignment="Left" Margin="38,204,0,0" Width="120" Content="greedy" VerticalAlignment="Top" Height="29"/>
            <CheckBox x:Name="chbBeacon" HorizontalAlignment="Left" Margin="38,0,0,181" Width="120" Content="random beacon" VerticalAlignment="Bottom" Height="29"/>
            <CheckBox HorizontalAlignment="Left" Margin="38,0,0,117" Width="120" Content="alfa beta" VerticalAlignment="Bottom" Height="29" Name="chbAlfaBeta" />
            <Label Margin="165,199,0,0" VerticalAlignment="Top" Height="29" Content="Ilość policjantów od/do" HorizontalAlignment="Left" Width="164"/>
            <TextBox x:Name="txbIloscGoniacych" Margin="339,200,0,0" VerticalAlignment="Top" Height="25" TextWrapping="Wrap" HorizontalAlignment="Left" Width="72"/>
            <Label Margin="165,0,247,181" VerticalAlignment="Bottom" Height="34" Content="liczba losowych wierzchołków"/>
            <TextBox x:Name="txbBeaconRandom" HorizontalAlignment="Right" Margin="0,0,172,188" VerticalAlignment="Bottom" Width="72" Height="25" TextWrapping="Wrap"/>
            <Label Margin="165,0,0,121" VerticalAlignment="Bottom" Height="29" Content="głębokość zanurzenia od/do" HorizontalAlignment="Left" Width="163" />
            <TextBox Height="27" HorizontalAlignment="Left" Margin="491,323,0,0" Name="txbAlfaBetaDepthMax" VerticalAlignment="Top" Width="72" />
            <TextBox Height="25" HorizontalAlignment="Left" Margin="491,199,0,0" Name="txbIloscGoniacychMax" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" />
            <Label Content="czas marszu" Height="28" HorizontalAlignment="Left" Margin="419,258,0,0" Name="label12" VerticalAlignment="Top" />
            <TextBox Height="27" HorizontalAlignment="Left" Margin="491,258,0,0" Name="txbCzasMarszu" VerticalAlignment="Top" Width="72" />
            
        </Grid>
    </Grid>
</Page>
